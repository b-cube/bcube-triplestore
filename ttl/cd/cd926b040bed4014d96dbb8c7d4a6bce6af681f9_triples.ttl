<urn:uuid:d63777a9-2dab-4b2f-9cd9-d4889e11a9bd> bcube:dateCreated "2015-11-01T10:25:26.984Z" ;
    bcube:hasMetadataRecord <urn:uuid:1055ab4b-fb02-410d-b883-f3ccacfc411d> ;
