<urn:uuid:9352480f-48b9-4830-b48b-ea41d5db089b> bcube:dateCreated "2015-10-18T22:11:19.391Z" ;
    bcube:hasMetadataRecord <urn:uuid:8fd43d36-69e8-4131-b1fb-3fd036d76447> ;
