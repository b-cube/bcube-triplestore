<urn:uuid:a8cabcde-5059-4612-991a-3e4803dce6e3> bcube:dateCreated "2015-07-13T14:47:29.711Z" ;
    bcube:hasMetadataRecord <urn:uuid:a5389148-5dd5-4de6-834d-fdceafc399e3> ;
