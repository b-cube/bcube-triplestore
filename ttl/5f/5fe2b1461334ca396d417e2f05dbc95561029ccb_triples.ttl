<urn:uuid:ce2a47de-70e9-4b2c-bcaf-35c6c835c886> bcube:dateCreated "2015-11-05T16:54:30.943Z" ;
    bcube:hasMetadataRecord <urn:uuid:3f6771c7-b2ed-449d-8cd8-dfbea0a51cde> ;
