<urn:uuid:8d3c92e8-a3ca-4878-80b4-60af597919b2> bcube:dateCreated "2015-08-04T17:56:55.241Z" ;
    bcube:hasMetadataRecord <urn:uuid:1c3fd897-a3a5-4627-8cb6-e6ddbc1f6ee9> ;
