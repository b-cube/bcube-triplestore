<urn:uuid:6c7ef4bd-fdb6-45d4-ba8b-e1be3df5dacc> bcube:dateCreated "2015-08-05T01:44:47.895Z" ;
    bcube:hasMetadataRecord <urn:uuid:ac79d330-c6e0-4619-bb0a-437bb9b620d5> ;
